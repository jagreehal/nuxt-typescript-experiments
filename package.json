{
  "name": "nuxt-typescript-experiments",
  "version": "1.0.0",
  "description": "nuxt-typescript-experiments",
  "scripts": {
    "dev": "node node_modules/.bin/nuxt",
    "build": "nuxt build --no-lock",
    "lint": "eslint --ext .ts,.js,.vue src/",
    "lint:fix": "eslint --ext .ts,.js,.vue src/ --fix",
    "pretty-quick": "pretty-quick",
    "start": "nuxt start",
    "test": "jest",
    "test:coverage": "jest --collectCoverage",
    "test-watch": "jest --watch"
  },
  "dependencies": {
    "@nuxt/typescript-build": "^0.2.10",
    "@nuxt/typescript-runtime": "^0.1.7",
    "@nuxtjs/axios": "^5.6.0",
    "@nuxtjs/eslint-config-typescript": "^0.1.3",
    "@nuxtjs/proxy": "^1.3.3",
    "@vue/composition-api": "^0.3.2",
    "axios": "^0.19.0",
    "dotenv": "^8.1.0",
    "graphql-tag": "^2.10.1",
    "nuxt": "^2.9.2",
    "tailwindcss": "^1.1.2"
  },
  "devDependencies": {
    "@nuxt/typescript": "^2.8.1",
    "@nuxtjs/eslint-config": "^1.1.2",
    "@nuxtjs/tailwindcss": "^1.1.2",
    "@testing-library/vue": "^3.0.0",
    "@types/jest": "^24.0.18",
    "@types/jest-axe": "^2.2.4",
    "@types/node": "^12.7.5",
    "@types/prettier": "^1.18.2",
    "@vue/cli-plugin-eslint": "^3.11.0",
    "@vue/cli-plugin-typescript": "^3.11.0",
    "@vue/cli-plugin-unit-jest": "^3.11.0",
    "@vue/cli-service": "^3.11.0",
    "@vue/eslint-config-prettier": "^5.0.0",
    "@vue/eslint-config-typescript": "^4.0.0",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-loader": "^8.0.6",
    "babel-preset-vue": "^2.0.2",
    "eslint": "^6.4.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-jest": "^22.17.0",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-vue": "^5.2.3",
    "jest": "^24.9.0",
    "jest-axe": "^3.2.0",
    "jest-vue-preprocessor": "^1.5.0",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "ts-jest": "^24.1.0",
    "ts-loader": "^6.1.2",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.3",
    "vue-jest": "^3.0.5",
    "wallaby-vue-compiler": "^1.0.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "pre-push": "npm run lint && npm test"
    }
  }
}
